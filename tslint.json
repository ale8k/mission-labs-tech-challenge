{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "no-console": false,
        "trailing-comma": false,
        "arrow-parens": false,
        "member-ordering": false,
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}